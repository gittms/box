namespace Definitif.Data.Test.Models
{
    ["Tables"]
	public model Table
	{
        ["Name"]
        public string Name;
	}

    ["Chairs"]
    public model Chair
    {
        ["TableId"]
        public foreign key Table Table;

        ["Name"]
        public string Name;
    }

    ["Chair2Chair"]
    public many to many model ChairToChair
    {
        ["FirstId"]
        public foreign key Chair First;

        ["SecondId"]
        public foreign key Chair Second;

        ["Owner"]
        public string Owner;
    }
}
