["Users"]
public model User
{
    ["Name"]
    public string Name;
}

["Achievements"]
public model Achievement
{
    ["Name"]
    public string Name;
}

["UserAchievements"]
public many to many model UserAchievement
{
    ["UserId"]
    public foreign key User User;

    ["AchievementId"]
    public foreign key Achievement Achievement;

    ["Received"]
    public DateTime Received;
}
